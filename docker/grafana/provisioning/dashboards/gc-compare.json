{
  "annotations": {
    "list": []
  },
  "title": "G1GC vs ZGC Comparison",
  "uid": "gc-compare-dashboard",
  "version": 3,
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "tags": ["gc", "comparison"],
  "panels": [
    {
      "title": "GC Pause Time (ms) - Lower is Better",
      "description": "Max pause duration - THE key metric. ZGC should be <1ms, G1GC 20-200ms",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "max(jvm_gc_pause_seconds_max{job=\"g1gc-app\"}) * 1000",
          "legendFormat": "G1GC - Max Pause",
          "refId": "A"
        },
        {
          "expr": "max(jvm_gc_pause_seconds_max{job=\"zgc-app\"}) * 1000",
          "legendFormat": "Generational ZGC - Max Pause",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "G1GC - Max Pause" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Generational ZGC - Max Pause" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "GC Pause Count (Total)",
      "description": "Cumulative GC pauses - ZGC has more frequent but tiny pauses",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(jvm_gc_pause_seconds_count{job=\"g1gc-app\"})",
          "legendFormat": "G1GC - Total Pauses",
          "refId": "A"
        },
        {
          "expr": "sum(jvm_gc_pause_seconds_count{job=\"zgc-app\"})",
          "legendFormat": "Generational ZGC - Total Pauses",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "G1GC - Total Pauses" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Generational ZGC - Total Pauses" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "Heap Memory Used (MB)",
      "description": "Memory consumption pattern",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum(jvm_memory_used_bytes{job=\"g1gc-app\", area=\"heap\"}) / 1024 / 1024",
          "legendFormat": "G1GC - Heap Used",
          "refId": "A"
        },
        {
          "expr": "sum(jvm_memory_used_bytes{job=\"zgc-app\", area=\"heap\"}) / 1024 / 1024",
          "legendFormat": "Generational ZGC - Heap Used",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "G1GC - Heap Used" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Generational ZGC - Heap Used" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "Process CPU Usage",
      "description": "CPU overhead - ZGC does concurrent work but should have lower spikes",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "avg(process_cpu_usage{job=\"g1gc-app\"})",
          "legendFormat": "G1GC - CPU",
          "refId": "A"
        },
        {
          "expr": "avg(process_cpu_usage{job=\"zgc-app\"})",
          "legendFormat": "Generational ZGC - CPU",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "G1GC - CPU" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Generational ZGC - CPU" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "GC Overhead (%) - Lower is Better",
      "description": "Percentage of time spent in GC pauses",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "sum(rate(jvm_gc_pause_seconds_sum{job=\"g1gc-app\"}[1m])) * 100",
          "legendFormat": "G1GC - Overhead %",
          "refId": "A"
        },
        {
          "expr": "sum(rate(jvm_gc_pause_seconds_sum{job=\"zgc-app\"}[1m])) * 100",
          "legendFormat": "Generational ZGC - Overhead %",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "G1GC - Overhead %" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Generational ZGC - Overhead %" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "HTTP Request Latency P99 (ms)",
      "description": "Tail latency - shows real app impact",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_requests_seconds_bucket{job=\"g1gc-app\", uri=~\"/api/.*\"}[1m]))) * 1000",
          "legendFormat": "G1GC - P99 Latency",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_requests_seconds_bucket{job=\"zgc-app\", uri=~\"/api/.*\"}[1m]))) * 1000",
          "legendFormat": "Generational ZGC - P99 Latency",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "G1GC - P99 Latency" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Generational ZGC - P99 Latency" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "Allocation Rate (MB/s)",
      "description": "Confirms equal load on both instances",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "targets": [
        {
          "expr": "sum(rate(jvm_gc_memory_allocated_bytes_total{job=\"g1gc-app\"}[1m])) / 1024 / 1024",
          "legendFormat": "G1GC - Allocation Rate",
          "refId": "A"
        },
        {
          "expr": "sum(rate(jvm_gc_memory_allocated_bytes_total{job=\"zgc-app\"}[1m])) / 1024 / 1024",
          "legendFormat": "Generational ZGC - Allocation Rate",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "MBs",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "G1GC - Allocation Rate" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Generational ZGC - Allocation Rate" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      }
    },
    {
      "title": "Summary Stats",
      "description": "Key numbers at a glance",
      "type": "stat",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "targets": [
        {
          "expr": "max(jvm_gc_pause_seconds_max{job=\"g1gc-app\"}) * 1000",
          "legendFormat": "G1GC Max Pause (ms)",
          "refId": "A"
        },
        {
          "expr": "max(jvm_gc_pause_seconds_max{job=\"zgc-app\"}) * 1000",
          "legendFormat": "Generational ZGC Max Pause (ms)",
          "refId": "B"
        },
        {
          "expr": "sum(jvm_gc_pause_seconds_count{job=\"g1gc-app\"})",
          "legendFormat": "G1GC Total Pauses",
          "refId": "C"
        },
        {
          "expr": "sum(jvm_gc_pause_seconds_count{job=\"zgc-app\"})",
          "legendFormat": "Generational ZGC Total Pauses",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "G1GC Max Pause (ms)" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Generational ZGC Max Pause (ms)" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "G1GC Total Pauses" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Generational ZGC Total Pauses" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "auto"
      }
    }
  ]
}