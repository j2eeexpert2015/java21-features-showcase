{
  "annotations": {
    "list": []
  },
  "title": "G1GC Detailed",
  "uid": "g1gc-detailed",
  "version": 7,
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "tags": [
    "gc",
    "g1gc",
    "jvm"
  ],
  "panels": [
    {
      "title": "GC Pause Time (ms)",
      "description": "Stop-the-world pause duration - G1GC typically 20-200ms",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "max(jvm_gc_pause_seconds_max{job=\"G1GC\", action!~\".*cycle.*\"}) * 1000",
          "legendFormat": "Max Pause",
          "refId": "A"
        },
        {
          "expr": "sum(rate(jvm_gc_pause_seconds_sum{job=\"G1GC\", action!~\".*cycle.*\"}[1m])) / sum(rate(jvm_gc_pause_seconds_count{job=\"G1GC\"}[1m])) * 1000",
          "legendFormat": "Avg Pause",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Max Pause"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Avg Pause"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "title": "GC Pause Count (Total)",
      "description": "Cumulative number of GC pauses",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(jvm_gc_pause_seconds_count{job=\"G1GC\", action!~\".*cycle.*\"})",
          "legendFormat": "Total Pauses",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "fixed",
            "fixedColor": "red"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        },
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value"
      }
    },
    {
      "title": "GC Pause Rate (per second)",
      "description": "Number of GC pauses per second",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(jvm_gc_pause_seconds_count{job=\"G1GC\", action!~\".*cycle.*\"}[1m]))",
          "legendFormat": "Pause Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Pause Rate"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "title": "Heap Memory Usage (MB)",
      "description": "Heap memory utilization - watch for sawtooth pattern",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum(jvm_memory_used_bytes{job=\"G1GC\", area=\"heap\"}) / 1024 / 1024",
          "legendFormat": "Heap Used",
          "refId": "A"
        },
        {
          "expr": "max(jvm_memory_max_bytes{job=\"G1GC\", area=\"heap\"}) / 1024 / 1024",
          "legendFormat": "Heap Max",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Heap Used"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Heap Max"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "dark-red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "title": "Process CPU Usage",
      "description": "JVM process CPU utilization",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "avg(process_cpu_usage{job=\"G1GC\"})",
          "legendFormat": "Process CPU",
          "refId": "A"
        },
        {
          "expr": "avg(system_cpu_usage{job=\"G1GC\"})",
          "legendFormat": "System CPU",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Process CPU"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "System CPU"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "title": "GC Overhead (%) - Lower is Better",
      "description": "Percentage of time spent in GC pauses",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum(rate(jvm_gc_pause_seconds_sum{job=\"G1GC\", action!~\".*cycle.*\"}[1m])) * 100",
          "legendFormat": "GC Overhead %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "GC Overhead %"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "title": "HTTP Request Latency P99 (ms)",
      "description": "Tail latency - shows real app impact",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_requests_seconds_bucket{job=\"G1GC\", uri=~\"/api/.*\"}[1m]))) * 1000",
          "legendFormat": "P99",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_requests_seconds_bucket{job=\"G1GC\", uri=~\"/api/.*\"}[1m]))) * 1000",
          "legendFormat": "P95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(http_server_requests_seconds_bucket{job=\"G1GC\", uri=~\"/api/.*\"}[1m]))) * 1000",
          "legendFormat": "P50",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "P99"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "P95"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "P50"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "yellow",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "title": "Allocation Rate (MB/s)",
      "description": "Memory allocation throughput",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "targets": [
        {
          "expr": "sum(rate(jvm_gc_memory_allocated_bytes_total{job=\"G1GC\"}[1m])) / 1024 / 1024",
          "legendFormat": "Allocation Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "MBs",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Allocation Rate"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "title": "GC Events by Cause",
      "description": "Why GC was triggered - Evacuation Pause, Humongous Allocation, etc.",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "targets": [
        {
          "expr": "sum by (cause) (increase(jvm_gc_pause_seconds_count{job=\"G1GC\", action!~\".*cycle.*\"}[1m]))",
          "legendFormat": "{{cause}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "bars",
            "lineWidth": 1,
            "fillOpacity": 50,
            "stacking": {
              "mode": "normal"
            }
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "G1 Evacuation Pause"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Metadata GC Threshold"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Humongous Allocation"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "yellow",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Preventive Collection"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "dark-red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "title": "G1GC Summary",
      "description": "Key metrics at a glance",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 32
      },
      "targets": [
        {
          "expr": "max(jvm_gc_pause_seconds_max{job=\"G1GC\", action!~\".*cycle.*\"}) * 1000",
          "legendFormat": "Max Pause (ms)",
          "refId": "A"
        },
        {
          "expr": "sum(jvm_gc_pause_seconds_count{job=\"G1GC\", action!~\".*cycle.*\"})",
