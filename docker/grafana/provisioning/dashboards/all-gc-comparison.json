{
  "annotations": {
    "list": []
  },
  "title": "All Three GCs Comparison",
  "uid": "gc-all-three-compare",
  "version": 1,
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "tags": [
    "gc",
    "comparison",
    "all"
  ],
  "panels": [
    {
      "title": "GC Pause Time (ms) - Lower is Better",
      "description": "Max pause duration comparison - G1GC vs Generational ZGC vs ZGC",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "max by (job) (jvm_gc_pause_seconds_max{action!~\".*cycle.*\"}) * 1000",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": "G1GC"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Generational-ZGC"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "^ZGC$"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
